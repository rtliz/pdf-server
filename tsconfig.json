{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "noImplicitThis":false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node"
    ],
  }
}